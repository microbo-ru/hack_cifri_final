{
  "openapi": "3.0.1",
  "info": {
    "title": "Государственные услуги в Республике Татарстан.",
    "description": "Предоставляет доступ к гос услугам Республики Татарстан",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8084"
    }
  ],
  "paths": {
    "/rosreestr": {
      "get": {
        "tags": [
          "rosreestr"
        ],
        "summary": "Получить кадастровый номер (внешний - заглушка)",
        "operationId": "getRosreestr",
        "parameters": [
          {
            "name": "address",
            "in": "path",
            "description": "Адрес объекта",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RosreestrResp"
                }
              }
            }
          },
          "404": {
            "description": "Request not found",
            "content": {}
          }
        },
        "x-swagger-router-controller": "Default"
      }
    },
    "/services": {
      "get": {
        "tags": [
          "services"
        ],
        "summary": "Возвращает список всех поддерживаемых услуг",
        "operationId": "getServices",
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Service"
                  },
                  "x-content-type": "application/json"
                }
              }
            }
          },
          "405": {
            "description": "Invalid input",
            "content": {}
          }
        },
        "x-swagger-router-controller": "Default"
      }
    },
    "/process": {
      "post": {
        "tags": [
          "BPMN"
        ],
        "summary": "Инициировать исполнение услуги (Работает только с постмана)",
        "operationId": "newProcess",
        "requestBody": {
          "description": "Initiates new request",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Request"
              }
            }
          },
          "required": true
        },
        "responses": {
          "405": {
            "description": "Invalid input",
            "content": {}
          }
        },
        "x-swagger-router-controller": "Default"
      }
    },
    "/process/{id}": {
      "get": {
        "tags": [
          "BPMN"
        ],
        "summary": "Получить BPMN описание конкретной услуги",
        "operationId": "getProcessById",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "Definition ID процесса",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Request"
                }
              }
            }
          },
          "404": {
            "description": "Request not found",
            "content": {}
          }
        },
        "x-swagger-router-controller": "Default"
      }
    }
  },
  "components": {
    "schemas": {
      "RosreestrResp": {
        "required": [
          "kadastrNumber"
        ],
        "type": "object",
        "properties": {
          "kadastrNumber": {
            "type": "string",
            "example": "12345:3455:33"
          }
        },
        "example": {
          "kadastrNumber": "12345:3455:33"
        }
      },
      "Service": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "expectedDuration": {
            "type": "integer"
          }
        },
        "example": {
          "name": "name",
          "id": "id",
          "expectedDuration": 0
        }
      },
      "Request": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "dateSubmitted": {
            "type": "string"
          },
          "serviceIdRequested": {
            "type": "string"
          },
          "status": {
            "type": "string"
          },
          "xmlState": {
            "type": "string"
          },
          "formData": {
            "type": "object"
          }
        },
        "example": {
          "serviceIdRequested": "serviceIdRequested",
          "formData": {},
          "id": "id",
          "xmlState": "xmlState",
          "dateSubmitted": "dateSubmitted",
          "status": "status"
        }
      }
    }
  }
}